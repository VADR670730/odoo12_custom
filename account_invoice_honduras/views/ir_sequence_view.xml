<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
   <record id="honduras_sequence_view" model="ir.ui.view">
            <field name="name">honduras_sequence_view.form.inherit</field>
            <field name="model">ir.sequence</field>
            <field name="inherit_id" ref="base.sequence_view"/>
            <field name="arch" type="xml">
                <xpath  expr="//field[@name='date_range_ids']//field[@name='number_next_actual']" position="after">
                    <field name="cai"/>
                </xpath>

                <xpath  expr="//field[@name='date_range_ids']//field[@name='number_next_actual']" position="after">
                    <field name="numero_final"/>
                </xpath>

               <xpath  expr="//field[@name='date_range_ids']//field[@name='number_next_actual']" position="after">
                    <field name="numero_inicial"/>
                </xpath>

            </field>
        </record>
       </data>
</odoo>