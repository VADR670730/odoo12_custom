<?xml version="1.0" encoding="utf-8"?>
<odoo>

        <record id="view_report_wizard_excel" model="ir.ui.view">
        <field name="name">ventas.excel.wizard.form</field>
        <field name="model">ventas.excel.wizard</field>
        <field name="arch" type="xml">
            <form string="Excel Report">
                <group>

                    <group>                       
                        <field name="fecha_inicio" />
                        <field name="fecha_fin" />
                         <field name="horario" />
                      </group>
                </group>
                <footer>
                    <button name="print_xls_report" string="Export" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>